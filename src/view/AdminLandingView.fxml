<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminLandingController">
   <children>
      <Label alignment="CENTER" layoutX="247.0" prefHeight="80.0" prefWidth="600.0" stylesheets="@headerStyleSheet.css" text="Admin" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font name="Times New Roman Bold" size="36.0" />
         </font>
      </Label>
      <Button fx:id="adminAddBookButton" layoutX="40.0" layoutY="128.0" mnemonicParsing="false" onAction="#handleAddBook" prefHeight="27.0" prefWidth="128.0" stylesheets="@buttonStyleSheet.css" text="Add Book" AnchorPane.leftAnchor="40.0" />
      <Button fx:id="adminDelBookButton" layoutX="38.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleDelBook" prefHeight="27.0" prefWidth="128.0" stylesheets="@buttonStyleSheet.css" text="Delete Book" />
      <Button fx:id="adminCheckBookButton" layoutX="39.0" layoutY="286.0" mnemonicParsing="false" onAction="#handleCheckBook" prefHeight="27.0" prefWidth="128.0" stylesheets="@buttonStyleSheet.css" text="Check Availability" />
      <Button fx:id="logoutButton" layoutX="217.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleLogout" stylesheets="@buttonStyleSheet.css" text="Logout" />
      <Button fx:id="adminCheckStudentButton" layoutX="400.0" layoutY="128.0" mnemonicParsing="false" onAction="#handleAcceptReturn" prefHeight="27.0" prefWidth="166.0" stylesheets="@buttonStyleSheet.css" text="Accept Return" />
      <Button fx:id="adminApproveButton" layoutX="400.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleCreate" prefHeight="23.0" prefWidth="166.0" stylesheets="@buttonStyleSheet.css" text="Create Student Account" />
      <Button fx:id="adminUpdateStudentButton" layoutX="403.0" layoutY="286.0" mnemonicParsing="false" onAction="#handleUpdateStudent" prefHeight="27.0" prefWidth="166.0" stylesheets="@buttonStyleSheet.css" text="Check/Update Student Account">
         <font>
            <Font size="10.0" />
         </font></Button>
      <Button fx:id="helpButton" layoutX="331.0" layoutY="350.0" mnemonicParsing="false" onAction="#handleHelp" stylesheets="@buttonStyleSheet.css" text="Help" />
      <Button layoutX="248.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleClear" stylesheets="@buttonStyleSheet.css" text="Clear Dues" />
   </children>
</AnchorPane>
