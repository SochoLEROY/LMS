<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="500.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StudentLandingController">
   <children>
      <Label prefHeight="70.0" prefWidth="501.0" stylesheets="@headerStyleSheet.css" text="                    Student">
         <font>
            <Font name="Times New Roman Bold Italic" size="36.0" />
         </font>
      </Label>
      <Button fx:id="studLandSearchButton" layoutX="205.0" layoutY="80.0" mnemonicParsing="false" onAction="#handleSearch" stylesheets="@buttonStyleSheet.css" text="Search books" />
      <Button fx:id="studLandDuesButton" layoutX="210.0" layoutY="137.0" mnemonicParsing="false" onAction="#handleDues" stylesheets="@buttonStyleSheet.css" text="Check Dues" />
      <Button fx:id="studLandRequestButton" layoutX="204.0" layoutY="193.0" mnemonicParsing="false" onAction="#handleRequest" stylesheets="@buttonStyleSheet.css" text="Request Issue" />
      <Button fx:id="studLandingLogout" layoutX="75.0" layoutY="246.0" mnemonicParsing="false" onAction="#handleLogout" stylesheets="@buttonStyleSheet.css" text="Logout" />
      <Button fx:id="helpButton" layoutX="382.0" layoutY="246.0" mnemonicParsing="false" onAction="#handleHelp" stylesheets="@buttonStyleSheet.css" text="Help" />
      <Label fx:id="userLabel" layoutX="10.0" layoutY="24.0" text="User: " textFill="WHITE" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="24.0">
         <font>
            <Font name="Times New Roman Bold" size="13.0" />
         </font>
      </Label>
      <Label fx:id="regLabel" layoutX="10.0" layoutY="51.0" text="Reg no: " textFill="WHITE" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="51.0">
         <font>
            <Font name="Times New Roman Bold" size="13.0" />
         </font></Label>
   </children>
</AnchorPane>
