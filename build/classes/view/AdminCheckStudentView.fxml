<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.admin.AdminCheckStudentController">
   <children>
      <Label layoutY="-1.0" prefHeight="80.0" prefWidth="600.0" stylesheets="@headerStyleSheet.css" text="                Check Student Details">
         <font>
            <Font name="Times New Roman Bold Italic" size="36.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="114.0" text="Reg Number:" />
      <Label layoutX="287.0" layoutY="186.0" text="Or">
         <font>
            <Font name="Times New Roman Bold Italic" size="24.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="252.0" text="User Id:" />
      <TextField fx:id="regField" layoutX="138.0" layoutY="109.0" prefHeight="27.0" prefWidth="325.0" promptText="Enter Registration number of Student" />
      <TextField fx:id="userField" layoutX="138.0" layoutY="247.0" prefHeight="27.0" prefWidth="325.0" promptText="Enter Library User Id of Student" />
      <Button fx:id="checkStudentSearchButton" layoutX="138.0" layoutY="346.0" mnemonicParsing="false" onAction="#handleSearch" stylesheets="@buttonStyleSheet.css" text="Search" />
      <Button fx:id="helpButton" layoutX="404.0" layoutY="346.0" mnemonicParsing="false" onAction="#handleHelp" stylesheets="@buttonStyleSheet.css" text="Help" />
      <Button fx:id="checkStudentCancelButton" layoutX="271.0" layoutY="346.0" mnemonicParsing="false" onAction="#handleCancel" stylesheets="@buttonStyleSheet.css" text="Cancel" />
   </children>
</AnchorPane>
